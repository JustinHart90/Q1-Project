{
  "manifest_version": 2,
  "key": "",
  "name": "HQ",
  "description": "This extension loads a custom dashboard when a user adds a new tab or window",
  "version": "1.1.4",
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "identity",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
  ],
  "icons": {
    "128": "img/HQ copy.png"
  },
  "browser_action": {
    "default_title": "New Tab Headquarters"
  },
  "chrome_url_overrides": {
    "newtab": "../views/index.hbs"
  },
  "oauth2": {
    "client_id": "556974443493-b9khrm41l2h66vjvm0l0tgq5qdh9vrls.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/drive"]
  }
}
