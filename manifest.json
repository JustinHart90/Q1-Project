{
  "manifest_version": 2,

  "name": "HQ",
  "description": "This extension loads a custom dashboard when a user adds a new tab or window",
  "version": "1.0",

  "icons": {
    "128": "html/HQ copy.png"
  },
  "browser_action": {
    "default_title": "HQ - New tab explorer"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "identity",
    "geolocation",
    "http://*.google.com/"
  ]
}
